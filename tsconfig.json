{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "target": "ES2015",
  "downlevelIteration": true,
  "compilerOptions": {
    "rootDir": ".",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist"
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ],
  "libs": ["es2015", "es2016", "es2017", "es2018", "es2019", "es2020", "esnext"]
}
